# PostgreSQL
POSTGRES_DB=go_platform
POSTGRES_USER=admin
POSTGRES_PASSWORD=admin
POSTGRES_PORT=5432
POSTGRES_HOST=postgres
POSTGRES_DSN=postgres://admin:admin@postgres:5432/go_platform?sslmode=disable

# General config
LOG_LEVEL=info
SERVER_PORT=8080
GRPC_PORT=50051

# Redis
REDIS_ADDR=platform_redis:6379
REDIS_PASSWORD=admin

# NATS
NATS_URL=nats://platform_nats:4222

# Dog API
DOG_API_BASE_URL=https://dog.ceo/api

# S3
S3_STORAGE_BUCKET=dogs
S3_STORAGE_KEY=minioadmin
S3_STORAGE_SECRET=minioadmin
S3_STORAGE_ENDPOINT=http://platform_minio:9002
S3_STORAGE_REGION=us-east-1
S3_STORAGE_ENDPOINT_PUBLIC=http://platform_minio:9002

# MySQL Configuration
MYSQL_DB=go_platform
MYSQL_USER=admin
MYSQL_PASSWORD=admin
MYSQL_ROOT_PASSWORD=admin
MYSQL_PORT=3306
MYSQL_HOST=mysql
MYSQL_DSN=admin:admin@tcp(mysql:3306)/go_platform?parseTime=true&loc=UTC

# ClickHouse Configuration
CLICKHOUSE_DB=go_platform
CLICKHOUSE_USER=admin
CLICKHOUSE_PASSWORD=admin
CLICKHOUSE_PORT=9000
CLICKHOUSE_HTTP_PORT=8123
CLICKHOUSE_HOST=clickhouse
CLICKHOUSE_DSN=clickhouse://admin:admin@clickhouse:9000/go_platform